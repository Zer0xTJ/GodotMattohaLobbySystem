{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Godot - Debug",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "C:\\Users\\me\\Documents\\Apps\\Godot\\Godot.exe",
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal",
			"stopAtEntry": false,
			"args": ["--path", "${workspaceRoot}", "--verbose"]
		}
	]
}
